        <link:labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="loc_DE5521" xlink:to="lbl_DE5521"/>
        <link:label xlink:type="resource" xlink:label="lbl_DE5521" xlink:role="http://sbr.gov.au/fdtn/sbr.01.02.tech/businessDefinition" xml:lang="en">This is the value, during the relevant period, of transfers of retained profits from participating business of the reporting party to Shareholders' funds.</link:label>
        <link:loc xlink:type="locator" xlink:href="bafpo4.02.06.data.xsd#DE5225" xlink:label="loc_DE5225"/>
