        <link:label xlink:type="resource" xlink:label="lbl_DM548" xlink:role="http://www.xbrl.org/2003/role/label" xml:lang="en">Derivative Financial Instrument Exposure Dimension</link:label>
        <link:labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="loc_DM548" xlink:to="lbl_DM548"/>
        <link:label xlink:type="resource" xlink:label="lbl_DM548" xlink:role="http://sbr.gov.au/fdtn/sbr.01.02.tech/businessDefinition" xml:lang="en">This dimension categorises the reported data according to what is the type of derivative financial instrument exposure.</link:label>
        <link:loc xlink:type="locator" xlink:href="DrvtvFnnclInstrmntExpsr.02.01.data.xsd#DV6145" xlink:label="loc_DV6145"/>
        <link:label xlink:type="resource" xlink:label="lbl_DV6145" xlink:role="http://www.xbrl.org/2003/role/label" xml:lang="en">Debt Funding Borrowings Derivative Exposures</link:label>
        <link:labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="loc_DV6145" xlink:to="lbl_DV6145"/>
        <link:label xlink:type="resource" xlink:label="lbl_DV6145" xlink:role="http://sbr.gov.au/fdtn/sbr.01.02.tech/businessDefinition" xml:lang="en">The value reported is the amount of any derivative exposure in relation to debt funding or borrowing.</link:label>
        <link:loc xlink:type="locator" xlink:href="DrvtvFnnclInstrmntExpsr.02.01.data.xsd#DV6146" xlink:label="loc_DV6146"/>
        <link:label xlink:type="resource" xlink:label="lbl_DV6146" xlink:role="http://www.xbrl.org/2003/role/label" xml:lang="en">Other Assets Derivative Exposures</link:label>
        <link:labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="loc_DV6146" xlink:to="lbl_DV6146"/>
        <link:label xlink:type="resource" xlink:label="lbl_DV6146" xlink:role="http://sbr.gov.au/fdtn/sbr.01.02.tech/businessDefinition" xml:lang="en">The value reported is the amount of any derivative exposure other than those classified as investment assets.</link:label>
        <link:loc xlink:type="locator" xlink:href="DrvtvFnnclInstrmntExpsr.02.01.data.xsd#DV6147" xlink:label="loc_DV6147"/>
        <link:label xlink:type="resource" xlink:label="lbl_DV6147" xlink:role="http://www.xbrl.org/2003/role/label" xml:lang="en">Claims Liabilities Derivative Exposures</link:label>
        <link:labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="loc_DV6147" xlink:to="lbl_DV6147"/>
        <link:label xlink:type="resource" xlink:label="lbl_DV6147" xlink:role="http://sbr.gov.au/fdtn/sbr.01.02.tech/businessDefinition" xml:lang="en">The value reported is the amount of any derivative exposure in relation to claims liabilities as determined in accordance with the relevant accounting standard. Claims liabilities represent the total estimated ultimate cost to an insurer of settling all claims arising from events which have occurred at the relevant date, whether or not reported.</link:label>
        <link:loc xlink:type="locator" xlink:href="DrvtvFnnclInstrmntExpsr.02.01.data.xsd#DV6148" xlink:label="loc_DV6148"/>
        <link:label xlink:type="resource" xlink:label="lbl_DV6148" xlink:role="http://www.xbrl.org/2003/role/label" xml:lang="en">Investment Portfolio Derivative Exposures</link:label>
        <link:labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="loc_DV6148" xlink:to="lbl_DV6148"/>
        <link:label xlink:type="resource" xlink:label="lbl_DV6148" xlink:role="http://sbr.gov.au/fdtn/sbr.01.02.tech/businessDefinition" xml:lang="en">The value reported is the amount of any derivative exposure that relates to the reporting insurer's investment assets, either as hedging a component of the underlying investments or as an outright investment.</link:label>
        <link:loc xlink:type="locator" xlink:href="DrvtvFnnclInstrmntExpsr.02.01.data.xsd#DV7999" xlink:label="loc_DV7999"/>
        <link:label xlink:type="resource" xlink:label="lbl_DV7999" xlink:role="http://www.xbrl.org/2003/role/label" xml:lang="en">All Derivative Exposures</link:label>
        <link:labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="loc_DV7999" xlink:to="lbl_DV7999"/>
        <link:label xlink:type="resource" xlink:label="lbl_DV7999" xlink:role="http://sbr.gov.au/fdtn/sbr.01.02.tech/businessDefinition" xml:lang="en">The information reported is in relation to the value of the amount of all derivative exposure.</link:label>
